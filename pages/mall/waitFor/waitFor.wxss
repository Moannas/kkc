page,.main{
    background: #EEEEEE;
    width: 100%;
    height: 100%;
    font-family:Adobe Heiti Std R;
}
.isbackground{
    width: 750rpx;
    height: 560rpx;
    background:linear-gradient(-43deg,rgba(42,20,246,1),rgba(228,9,249,1));
}
.isauto{
    position: fixed;
    top: 0;
    left: 30rpx;
    width: 690rpx;
    height: 100%;
    background: none;
    overflow-y: scroll;
}
.isauto::-webkit-scrollbar {
    display: none;
}
.title{
    background: none;
    margin: 30rpx 0 14rpx 0;
    color: #fff;
    display: flex;
    align-items: center;
}
.waitFor_tex{
    font-size: 40rpx;
    margin-right: 12rpx
}
.title span{
    width:10rpx;
    height:10rpx;
    display: block;
    background:rgba(244,244,244,1);
    border-radius:50%;
    margin: 0 5rpx
}
.unpaid{
    width: 100%;
    height: 951rpx;
    border-radius: 15rpx;
    margin-bottom: 20rpx;
}
.unpaid_tex{
    width: 650rpx;
    height: 100%;
    margin: auto;
}
.top_tex{
    width: 100%;
    height: 86rpx;
    border-bottom: 1rpx solid #ECECEC;
    line-height: 86rpx;
    display: flex;
    align-items: center;
}
.drinks_tex{
    font-size:32rpx;
    font-weight:bold;
    color:rgba(2,4,51,1);
    line-height:80rpx;
}
.room{
    font-size:28rpx;
    font-weight:bold;
    color:rgba(102,102,102,1);
    line-height:80rpx;
}
.top_tex .icons{
    width: 30rpx;
    height: 30rpx;
    display: block;
    background-size: cover;
    background-image:url("data:image/png;base64,");
    margin:0 15rpx;
}
.list{ 
    position: relative; 
    height: 130rpx; 
    border-bottom: 1rpx solid #ECECEC;
    overflow: visible; 
} 
.list image{
    width: 101rpx;
    height: 89rpx;
    float: left;
    border-radius: 5rpx;
    margin-right: 15rpx
}
.goods{
    width: 435rpx;
    float: left;
    font-family:PingFang-SC-Medium;
}
.goods view{
    font-size:28rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
    line-height:36rpx;
}
.goods p{
    font-size:24rpx;
    font-weight:500;
    color:rgba(153,153,153,1);
    line-height:36rpx;
}
.fr_num{
    float: right;
    width:100rpx ;
    height: 100%;
}
.fr_num>p{
    font-size: 24rpx;
    color: #FF7C00;
}
.fr_num view{
    width: 100%;
    font-size: 24rpx;
    color: #666;
    text-align: right;
    margin-top: 20rpx;
}
.add_num{
    display: flex;
    align-items: center;
}
.add_num p{
    margin: 0 10rpx
}
.add_num span{
    width: 22rpx;
    height: 22rpx;
    background-size: cover;
    display: block;
}
.add_num span:nth-child(1){
    background-image: url("data:image/png;base64,");
}
.add_num span:nth-child(3){
    background-image: url("data:image/png;base64,");
}
.cont{ 
    width: 100%; 
    margin: 0 auto; 
} 
/* .list_del{ 
    position: absolute; 
}  */
.list_del.txt{ 
    position: relative;
    background: #fff; 
    padding:20rpx 0 ;
    width: 100%; 
    height: 100%-40rpx;
    z-index: 5; 
    transition: left 0.2s ease-in-out; 
    white-space:nowrap; 
    overflow:hidden; 
    text-overflow:ellipsis; 
} 
.remove{ 
    background: #e64340!important; 
    font-size: 24rpx;
    width: 91rpx;
    height: 100%;
    display: flex;  
    justify-content: center;  
    align-items: center;  
    z-index: 10000; 
    position: absolute;  
    top: 0;  
    right: -91rpx;  
    color: #fff 
}
.total_tex{
    width: 102rpx;
    margin: 0 auto;
    height: 37rpx;
    text-align: center;
    font-size:22rpx;
    color:rgba(153,153,153,1);
    display: flex;
    align-items: center
}
.total_tex span{
    margin-left: 10rpx;
    width: 24rpx;
    height: 14rpx;
    background-size: cover;
    background-image: url("data:image/png;base64,");
    display: block
}
.discount{
    margin: 40rpx 0 30rpx 0
}
.discount2{line-height: 36rpx;}
.discount p:nth-child(1),.discount2 p:nth-child(1){
    float: left;
    font-size:26rpx;
    color:rgba(51,51,51,1);
}
.discount p:nth-child(2){
    float: right;
    font-size:24rpx;
    color:rgba(253,95,38,1);
}
.discount2 p:nth-child(2){
    float: right;
    font-size:36rpx;
    color:rgba(253,95,38,1);
}
.cancel_but{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 98rpx;
    background: #C000FF;
    border-radius: 0;
    font-size: 36rpx;
    color: #fff;
}
@media all and (min-width:768px) {
    .discount{
        margin: 0
    }
}





















