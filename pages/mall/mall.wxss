@import "../choosed/choosed.wxss";

button {
    padding: 0
}

button[disabled][type="default"],
wx-button[disabled]:not([type]) {
    background: url(data:image/png;base64,) no-repeat !important;
    background-size: cover !important;
}

.mall-container {
    padding-bottom: 90rpx;
    background-color: #f3f3f3
}

.head-tab {
    height: 92rpx;
    background-color: #f3f3f3;
}

.head-item {
    width: auto;
    height: 92rpx;
    line-height: 92rpx;
    background-color: #f3f3f3
}

.list {
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 92rpx 42rpx 0;
    background-color: #fff
}

.list-less {
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 92rpx 42rpx 0;
    background-color: #fff
}

.goods {
    flex-shrink: 0;
    padding-top: 32rpx;
    margin: 0 0 44rpx;
    width: 312rpx
}

.cover {
    position: relative;
    width: 312rpx;
    height: 356rpx;
    border-radius: 16rpx;
    box-shadow: 0 0 10px #999
}

.name {
    margin: 25rpx 0 22rpx;
    width: 100%;
    text-align: center
}

.cover-wrapper {
    position: relative;
    justify-content: flex-start;
    height: 402rpx
}

.icon-mall-add {
    position: absolute;
    bottom: 0
}

.icon-goods-amount {
    position: absolute;
    bottom: 60rpx;
    right: 105rpx;
}

.bottom {
    position: fixed;
    justify-content: flex-end;
    bottom: 0;
    width: 100%;
    height: 88rpx;
    background-color: #838383
}

.total,
.price,
.sub {
    height: 88rpx;
    line-height: 88rpx;
    background-color: #838383;
    color: #fff
}

.total {
    margin-right: 40rpx
}

.price {
    font-weight: bold;
    font-size: 34rpx
}

.sub {
    width: 200rpx;
    background-color: #0192ff !important;
    text-align: center;
    font-size: 34rpx
}

.icon-logo {
    position: fixed;
    bottom: 20rpx;
    left: 30rpx;
    z-index: 99
}

.icon-total-amount {
    position: fixed;
    bottom: 87rpx;
    left: 91rpx;
    z-index: 99
}

.order-wrapper {
    position: fixed;
    bottom: 88rpx;
    width: 100%;
}

.order-wrapper .head {
    justify-content: space-between;
    padding: 0 40rpx 0 30rpx;
    height: 74rpx;
}

.order-wrapper .head,
.order-wrapper .head view,
.order-wrapper .head text {
    background-color: #dfdfdf
}

.order-wrapper .head text {
    font-size: 26rpx;
    color: #676767
}

.cart {
    padding: 0 30rpx 0;
    max-height: 536rpx;
    overflow-y: scroll
}

.cart .c-item {
    justify-content: space-between;
    height: 104rpx;
    border-bottom: 1px solid #ececec
}

.cart .c-item:last-child {
    padding-bottom: 12rpx;
    border: 0
}

.cart .c-item .num {
    /* margin: 0 28rpx 0; */
    width: 70rpx;
    text-align: center
}

.cart .c-item .c-name {
    color: #000
}

.c-name {
    font-size: 26rpx
}

.cart .c-item .c-price {
    margin-right: 34rpx;
    font-weight: bold
}
.hint_window{
    height: 85rpx;
    width: 100%;
    background:rgba(255,149,0,1);
    opacity:0.8;
    position: fixed;
    top: 92rpx;
    left: 0;
    z-index: 1000;
    color: #fff;
    font-size:24rpx;
    display: flex;
    align-items: center;
    justify-content: center
}
.hint_window span{
    width: 34rpx;
    height: 36rpx;
    background-image: url("data:image/png;base64,");
    background-size: cover;
    margin-right: 11rpx
}
.window_background{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: black;
    opacity: 0.5;
    z-index: 2000;
}
.window_main{
    position: fixed;
    top: 0;
    left: 0;
    width: 400rpx;
    height: 450rpx;
    background: #fff;
    z-index: 3000;
    border-radius: 25rpx;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    overflow: visible;
}
.w_main{
    border-radius: 25rpx;
    position: relative;
    width: 100%;
    height: 100%;
    overflow: visible;
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction:column
}
.w_main image{
    width: 235rpx;
    height: 204rpx;
    position: absolute;
    left: 50%;
    top: -25rpx;
    transform: translate(-50%,0);
    z-index: 4000;
}
.window_tex p{
    display: block;
}
.window_tex p:nth-child(1){
    margin: 130rpx 0 20rpx 0;
    font-size:36rpx;
    font-weight:500;
    color:#333333;
}
.window_tex span{
    font-size:24rpx;
    font-weight:500;
    color:rgba(125,28,234,1);
}

.w_main button{
    width:240rpx;
    height:80rpx;
    background:rgba(125,28,234,1);
    border-radius:40rpx;
    color: #fff;
    font-size: 32rpx;
    margin-top: 29rpx
}




