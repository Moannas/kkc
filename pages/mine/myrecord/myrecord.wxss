page{
    background: #EEEEEE;
    width: 100%;
    height: 100%;
}

.main{
    background: #EEEEEE;
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.isbackground{
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 273rpx;
    background: linear-gradient(to right, #811FEB, #C24EFC 100%);
}
.ispadding{
    padding: 34rpx 30rpx 85rpx 30rpx;
    width: 100%;
    height: 100%;
    background: none
}
.title{
    font-size: 30rpx;
    color: #fff;
}
.hint{
    font-size: 22rpx;
    color: #fff;
    font-weight:400;
    color:rgba(255,255,255,1);
    margin: 18rpx 0 25rpx 0;
    opacity:0.5;
    display: flex;
    align-items: center;
    background: none
}
.icon{
    width: 36rpx;
    height: 36rpx;
    display: inline-block;
    background-size: cover;
    background-image: url("data:image/png;base64,");
    margin-right: 11rpx 
}
.have{
    background: none
}
.have>view{
    height: 130rpx;
    width: 626rpx;
    background: #fff;
    border-radius: 10rpx;
    margin: 20rpx 0;
    padding:0 10rpx 0 30rpx;
    display: flex;
    align-items: center
}        
.isfl{
    flex: 1
}
.flex{
    flex: 2
}
.isfl view{
    line-height: 23rpx;
    display: flex;
    align-items: center;
    margin-top: 10rpx
}
.isfl .grade{
    font-size: 15rpx;
    color: #FFB300;
    border: 1rpx solid #FFB300;
    padding: 0 4rpx;
    margin-right: 10rpx
}
.name{
    font-size: 24rpx;
    color:rgba(0,0,0,1);
    opacity:0.5;
    margin-right: 10rpx
}
.timer{
    font-size: 24rpx;
    color: #FFB300
}   
.song{
    height: 28rpx;
    font-size: 30rpx;
}
.isfr{
    flex: 1;
    display: flex;
    align-items: center;
    text-align: right
}
.num{
    color: #FFB300;
    margin-right:36rpx;
}
.but1,.but2,.but3{
    width: 56rpx;
    height: 56rpx;
    margin-right: 15rpx;
    background-size: cover;
    border-radius:  50%;
    border: 0;
    padding: 0 !important;
}
.but3{
    margin-right:0;
    background-image: url("data:image/png;base64,")
}
.but2{
    background-image: url("data:image/png;base64,")
}
.but1{
    background-image: url("data:image/png;base64,")
}
.nolist{
    width: 400rpx;
    height: 678rpx;
    background: #fff;
    border-radius: 15rpx;
    padding: 177rpx 145rpx 0 145rpx;
    text-align: center;
    color: #999999;
}
.nolist image{
    width: 400rpx;
    height: 326rpx;
    margin-bottom:122rpx; 
}
.nolist view:nth-child(2){
    font-size: 30rpx;
    margin-bottom:20rpx ;
    color: #999999;
}
.nolist view:nth-child(3){
    font-size:24rpx;
    color: #999999;
}
 
















