page,.main{
    background: #EEEEEE;
    width: 100%;
    height: 100%;
    font-family:Adobe Heiti Std R;
}
.isbackground{
    width: 750rpx;
    height: 453rpx;
    background: linear-gradient(to right, #8A26ED, #CC55FF 100%);
}
.isauto{
    position: fixed;
    top: 0;
    left: 30rpx;
    width: 690rpx;
    height: 100%;
    background: none;
    overflow-y: scroll;
}
.isauto::-webkit-scrollbar {
    display: none;
}
.title{
    background: none;
    margin: 39rpx 0 30rpx 0;
    color: #fff;
}
.tex{
    font-size: 40rpx;
}
.title span{
    margin:0 20rpx;
    width: 31rpx;
    height: 31rpx;
    background-size: cover;
    display: inline-block;
    background-image: url("data:image/png;base64,")
}
.room{
    font-size: 24rpx;
}
.unpaid,.distribution_ing{
    height: 849rpx;
    border-radius: 15rpx;
    margin-bottom: 20rpx;
}
.unpaid_tex{
    width: 650rpx;
    height: 100%;
    margin: auto;
}
.top_tex{
    width: 100%;
    height: 86rpx;
    border-bottom: 1rpx solid #ECECEC;
    line-height: 86rpx;
}
.top_tex .icons{
    width: 30rpx;
    height: 30rpx;
    background-size: cover;
    background-image:url("data:image/png;base64,");
    margin-right: 17rpx;
    float: left;
    margin-top: 29rpx
}
.timer{
    font-size: 24rpx;
    color: #666666;
}
.state{
    font-size: 24rpx;
    color: #FF7C00;
    float: right
}
.list{
    padding: 20rpx 0;
    border-bottom: 1rpx solid #ECECEC
}
.list image{
    width: 101rpx;
    height: 89rpx;
    float: left;
    border-radius: 5rpx;
    margin-right: 15rpx
}
.goods{
    width: 435rpx;
    float: left;
    font-family:PingFang-SC-Medium;
}
.goods view{
    font-size:28rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
    line-height:36rpx;
}
.goods p{
    font-size:24rpx;
    font-weight:500;
    color:rgba(153,153,153,1);
    line-height:36rpx;
}
.fr_num{
    float: right;
}
.fr_num p{
    display: block
}
.fr_num p:nth-child(1){
    font-size: 24rpx;
    color: #FF7C00;
}
.fr_num p:nth-child(2){
    width: 100%;
    font-size: 24rpx;
    color: #666;
    text-align: right;
    margin-top: 20rpx;
}
.total_tex button{
    float: right;
    width: 155rpx;
    height: 58rpx;
    font-size:28rpx;
    font-weight:500;
    color:rgba(88,90,88,1);
    line-height: 58rpx;
    text-align: center;
    padding: 0;
}
.total_tex{
    font-size: 28rpx;
    color: #666;
    margin-top: 20rpx
}
.nolist{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction:column;
    height: 80%;
    border-radius: 20rpx
}
.nolist image{
    width: 374rpx;
    height: 293rpx;
}
.nolist view{
    font-size:30rpx;
    font-weight:500;
    color:rgba(153,153,153,1);
    line-height:50rpx;
}




