.bottom {
    width: 710rpx;
    height: 720rpx;
    border-radius: 30rpx;
    position: absolute;
    top: 365rpx;
    background: #fff;
    left: 50%;
    transform: translate(-50%, 0);
}
.bottom view{
    background: none
}
