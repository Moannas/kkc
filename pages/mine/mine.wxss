.main,.mine-container,.top {
    overflow: visible;
}

.yijian {
    width: 80rpx;
    height: 80rpx;
    background: #5b0fde;
    border-radius: 50% 0 0 50%;
    position: fixed;
    top: 91rpx;
    right: 0;
    text-align: center;
}
.yijian span {
    width: 45rpx;
    height: 36rpx;
    background-image: url("data:image/png;base64,");
    background-size: cover;
    display: inline-block;
    margin-top: 26rpx;
}
.top {
    width: 200%;
    height: 523rpx;
    background: linear-gradient(to right, #d80af9, rgb(50, 12, 243) 100%);
    border-radius: 0 0 50% 50%;
    position: relative;
    top: 0;
    left: 50%;
    transform: translate(-50%, 0);
}
.centre {
    position: absolute;
    top: 0;
    left: 50%;
    transform: translate(-50%, 0);
    text-align: center;
    background: none;
}
.head {
    width: 162rpx;
    height: 162rpx;
    display: block;
    border-radius: 50%;
    background: #c987fc;
    margin: 43rpx auto 20rpx auto;
    position: relative;
}
.head image {
    width: 150rpx;
    height: 150rpx;
    display: block;
    border-radius: 50%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}
.header-img {
    overflow:hidden;  
    display: block; 
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}

.nick-name {
    margin-bottom: 19rpx;
    font-size: 28rpx;
    color: #fff
}
.name {
    font-size: 28rpx;
    color: #fff;
    margin-bottom: 19rpx;
}
.roomviewnk{
    margin-top: 10rpx;
