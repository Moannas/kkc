page{
    background: #EEEEEE;
    width: 100%;
    height: 100%;
}
.main{
    background: #EEEEEE;
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.isbackground{
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 273rpx;
    background: linear-gradient(to right, #811FEB, #C24EFC 100%);
}
.ispadding{
    padding:0 0 85rpx 30rpx;
    width: 100%;
    height: 100%;
    background: none
}
.titlebox,.hint,.have view{
    background: none
}
.have{
    background: none
}
.isimgs{
    width: 36rpx;
    height: 35rpx;
    display: inline-block;
    background-image: url("data:image/png;base64,");
    background-size: cover;
    margin-right:11rpx
}
.title{
    font-size: 30rpx;
    color: #fff;
}
.titlebox{
    float: left;
    display: flex;
    align-items: center
}
.hint{
    width: 201rpx;
    height: 65rpx;
    border-radius: 30rpx;
    border: 1rpx solid rgba(255,255,255,0.5);
    text-align: center;
    float: right;
    font-size: 24rpx;
    font-weight:400;
    color:rgba(255,255,255,1);
    display: flex;
    align-items: center;
}
.icon{
    width: 36rpx;
    height: 36rpx;
    display: inline-block;
    background-size: cover;
    background-image: url("data:image/png;base64,");
    margin:0 11rpx 0 28rpx
}
.have{
    clear: both;
}
.have>view{
    height: 130rpx;
    width: 626rpx;
    border-radius: 10rpx;
    margin: 20rpx 0;
    padding:0 30rpx;
    overflow: hidden;
    background:rgba(255,255,255,1);
    box-shadow:0px 3px 5px 0px rgba(0, 0, 0, 0.2);
    display: flex;
    align-items: center
}        
.isfl{
    float: left;
    width: 550rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    flex-direction: column
}
.isfl view{
    display: flex;
    align-items: center;
    margin-top: 10rpx
}
.isfl .grade{
    font-size: 15rpx;
    color: #FFB300;
    border: 1rpx solid #FFB300;
    padding: 0 3rpx;
    margin-right:10rpx
}
.name{
    font-size: 24rpx;
    color:rgba(0,0,0,1);
    opacity:0.5;
}
.timer{
    font-size: 24rpx;
    color: #FFB300
}   
.song{
    font-size: 30rpx;
}
.isfr{
    float:right;
    margin-top: 10rpx
}

.but3{
    width: 56rpx;
    height: 56rpx;
    border: none;
    float: right;
    background-size: cover;
    background-image: url("data:image/png;base64,")
}

.nolist{
    float: left;
    margin-top: 40rpx;
    width: 400rpx;
    height: 678rpx;
    background: #fff;
    border-radius: 15rpx;
    padding: 177rpx 145rpx 0 145rpx;
    text-align: center;
    color: #999999;
}
.nolist image{
    width: 400rpx;
    height: 326rpx;
    margin-bottom:122rpx; 
}
.nolist view:nth-child(2){
    font-size: 30rpx;
    margin-bottom:20rpx ;
    color: #999999;
}
.nolist view:nth-child(3){
    font-size:24rpx;
    color: #999999;
}
 /* 弹窗 */
 .cancel_windows{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    background: none
 }
 .black{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: black;
    opacity: 0.5;
 }
.centre{
    width: 545rpx;
    height: 380rpx;
    background: #fff;
    border-radius: 20rpx;
    text-align: center;
    position: relative;
    overflow: visible 
}
.nocancel{
    font-size: 28rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
    margin-top: 111rpx
}
.cancel_p{
    font-size:24rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:rgba(102,102,102,1);
    line-height:9px;
}
.check_box{
    width: 150rpx;
    height: 35rpx;
    font-size: 24rpx;
    margin:24rpx 0 32rpx 78rpx;
}
.ministar{
    width: 37rpx;
    height: 86rpx;
    position: absolute;
    left: 253rpx;
    top: 196rpx;
    background-image: url("data:image/png;base64,");
    background-size: cover;
}
checkbox{
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    margin-right: 20rpx
}
checkbox .wx-checkbox-input {
    width: 24rpx;
    height: 24rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 22rpx;
    height: 22rpx;
    line-height: 28rpx;
    text-align: center;
    font-size: 24rpx;
}
.cancel_but,.continue_but{
    width: 240rpx;
    height: 80rpx;
    border-radius: 40rpx;
    float: left;
}
.cancel_but{
    background: #7D1CEA;
    margin:0 5rpx 0 30rpx;
    color: #fff;
    font-size: 28rpx;
    line-height: 80rpx
}
.continue_but{
    border: 2rpx solid #ccc;
    color: rgb(116, 114, 114);
    font-size: 28rpx;
    line-height: 80rpx
}
.star{
    width: 235rpx;
    height: 206rpx;
    position: absolute;
    top: -89rpx;
    left: 155rpx;
    z-index: 100;
}
.boat{
    width: 159rpx;
    height: 84rpx;
    position: absolute;
    left: 188rpx;
    bottom: -72rpx;
}






