/* 显示的内容 */
.feed-cont1{
    width: 100%;
    height: 100%;
    background: #fff;

}
.feed-bg{
    width: 100%;
    height: 440rpx;
    position: relative;
}
.feed-logo{
    position: absolute;
    top: 113rpx;
    left: 56rpx;
    z-index: 1200;
    width: 643rpx;
    height: 305rpx;
}
.form{
    width: 100%;
    overflow: hidden;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.form-li{
    width:100%;
    height: 89rpx;
    margin-top: 30rpx;
    background:rgba(238,238,238,1);
    border:2px solid rgba(216,216,216,1);
    opacity: 0.6;
    border-radius:10px;
    display: flex;
}
.form-li view{
    background: none;
    flex: 1;
    margin-left: 20rpx;
    height: 89rpx;
    line-height: 89rpx;
    font-size:28rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:#000;
}
.form-li input{
    flex: 9;
    background: none;
    font-size:28rpx;
    height: 89rpx;
    line-height: 89rpx;
}
.form-li:nth-child(4){
    height: 200rpx;
    box-sizing: border-box;
    margin-bottom: 20rpx;
    padding-right: 10rpx;
    flex-direction: column;
}
.form-li:nth-child(4) view{
    flex: 5;
    margin-bottom: 10rpx;
   
}
.form-li:nth-child(4) textarea{
    flex: 7.5;
    margin-left: 20rpx;
    font-size:28rpx;
}
.feed-submit{
    width: 90%;
    height:88rpx;
    background:linear-gradient(0deg,rgba(94,34,234,1),rgba(120,45,228,1));
    border-radius:10rpx;
    color: #fff;
}
.hover{
    width: 90%;
    height:88rpx;
    background:linear-gradient(0deg,rgba(94,34,234,1),rgba(120,45,228,1));
    border-radius:10rpx;
    opacity: 0.8;
    color: #fff;
}



