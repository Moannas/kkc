/**index.wxss**/

@import "../template/tl-banner/tl-banner.wxss";

button {
    margin: 0
}

button::after {
    width: 0;
    height: 0
}

button[disabled][type="default"],
wx-button[disabled]:not([type]) {
    background-color: #fff !important
}

.nav {
    justify-content: space-between;
    padding: 0 46rpx 0 30rpx;
    height: 128rpx;
}

.nav .tag {
    padding: 0;
    margin-left: 26rpx;
    font-size: 45rpx;
    background-color: #fff
}

.options {
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 0 22rpx;
    height: 162rpx;
}

.options .wrapper {
    width: 100rpx;
    margin-right: 20rpx;
}

.recommend .title {
    padding-left: 46rpx;
    height: 88rpx;
    line-height: 88rpx;
    border-bottom: 1px solid #ddd
}

/* 键盘 */
.checkCode-img {
    padding-top: 20rpx;
    text-align: center;
    background-color: rgba(0, 0, 0, 0)
}

.checkCode-bg {
    vertical-align: middle;
    width: 614rpx;
    height: 269rpx;
}

.input-container {
    display: flex;
    width: 660rpx;
    margin: 0 auto;
    margin-top: 50rpx;
    background-color: #fff;
}

.input-container view {
    width: 110rpx;
    height: 80rpx;
    margin: 0 17rpx;
    border-bottom: 5rpx solid #46b9fe;
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
}

.keyboard {
    width: 100%;
    display: flex;
    background-color: #fff;
    border-top: 1px solid #eee;
    margin-top: 50rpx;
}

.keyboard .number {
    display: flex;
    width: 100%;
    flex-wrap: wrap;
}

.keyboard .number view {
    width: 250rpx;
    height: 120rpx;
    line-height: 120rpx;
    text-align: center;
    box-sizing: border-box;
    border-right: 1px solid #eee;
    border-bottom: 1px solid #eee;
}

.keyboard .number view:nth-child(3n) {
    border-right: 0
}

.keyboard .number .item {
    font-size: 46rpx;
    font-weight: bold;
}

.keyboard .number .ctr-btn-item {
    font-size: 32rpx;
    font-weight: bold;
}

.keyboard .active {
    background-color: #e4e7ed;
}

.activity {
    background-color: #fff;
    padding: 25rpx 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 24rpx;
    border-bottom: 1px solid #f6f6f6;
}

.activity.activity-item {
    font-size: 26rpx;
}

/* 键盘结束 */
/*新增搜索样式*/
.top-search{
    width: 100%;
    height: 140rpx;
    box-sizing: border-box;
    padding-top: 30rpx;
    padding-left: 30rpx;
}
.top-search-left{
    float: left;
    width:521rpx;
    height:80rpx;
    background:rgba(238,238,238,1);
    border-radius:40rpx;
    margin-right: 15rpx;
}
.top-search-left-icon{
    margin-top: 20rpx;
    margin-left: 30rpx;
    margin-right: 5rpx;
    display: block;
    float: left;
    width: 36rpx;
    height: 36rpx;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: contain;
}
.top-search-left input{
    float: left;
    font-size: 28rpx;
    width: 445rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: none;
}
.top-search-right{
    width:159rpx;
    height:80rpx;
    background:rgba(120,102,248,0.1);
    border:1rpx solid rgba(255,255,255,1);
    border-radius:40rpx;
    margin-left: 10rpx;
}
.top-search-right-yidian{
    display: block;
    float: left;
    width: 40rpx;
    height: 40rpx;
    margin-left: 25rpx;
    margin-top: 20rpx;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: cover;
}
.top-search-right-wenzi{
    float: left;
    display: block;
    width:55rpx;
    height:27rpx;
    font-size:28rpx;
    margin-left: 15rpx;
    margin-top: 20rpx;
    font-family:PingFang-SC-Regular;
    font-weight:bold;
    color:rgba(120,102,248,1);
}
/*弹窗*/
/* 弹窗 */
.fixedWindows {
    background: none;
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 10000;
}

.OpenWindows {
    background: none;
    position: relative;
    width: 100%;
    height: 100%;
}

.Openback {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: black;
    opacity: 0.8;
}

.centerwindow {
    position: absolute;
    bottom: 60rpx;
    left: 0;
    right: 0;
    padding-top: 40rpx;
    margin: 0 auto;
    width: 710rpx;
    height: 246rpx;
    background: white;
    border-radius: 6px;
    z-index: 11000;
}

.windowText {
    box-sizing: border-box;
    padding: 0 50rpx;
    width: 100%
}

.tex {
    width: 100%;
    height: 88rpx;
    text-align: center;
    font-size: 32rpx
}

.windowBtn {
    position: absolute;
    display: flex;
    bottom: 0;
    width: 100%;
    border-top: 1rpx solid #ccc
}

button::after {
    border: none;
    border-radius: 0
}

.btn {
    width: 49%;
    height: 80rpx;
    border: 0;
    border-radius: 6px;
    font-size: 32rpx;
    background-color: #fff
}

.v_line {
    width: 1rpx;
    height: 80rpx;
    background: #ccc
}

.sure {
    color: rgb(6, 250, 6)
}